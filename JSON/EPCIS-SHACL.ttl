epcis:EventListShape
    a sh:NodeShape;
    sh:targetObjectsOf epcis:eventList;
    sh:property epcis:EventType_Constraint .
    

epcis:EventType_Constraint
    sh:path rdf:type;
    sh:in epcis:EventTypeEnum;
    sh:message "eventList must contain valid subclasses of EPCISEvent".


epcis:EventTypeEnum owl:sameAs (
   epcis:ObjectEvent
   epcis:AggregationEvent
   epcis:TransactionEvent
   epcis:TransformationEvent
   epcis:AssociationEvent
).


