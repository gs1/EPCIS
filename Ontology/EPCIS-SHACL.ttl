.



epcis:EventList_TypeConstraint 
    sh:path epcis:eventList ;
    sh:message "eventList must be specified within epcisBody";
    
    sh:minCount 0 .
    


epcis:EPCISDocumentShape 
    a sh:NodeShape;
    sh:targetClass epcis:EPCISDocument  ;
    sh:property epcis:schemaVersion_IsMandatory;
    sh:property epcis:creationDate_IsMandatory ;
    sh:property epcis:epcisBody_IsMandatory .
   
    
    
epcis:schemaVersion_IsMandatory 
    sh:path owl:versionInfo ;
    sh:minCount 1;
    sh:maxCount 1;
    sh:datatype  xsd:string;
    sh:message "schemaVersion must be specified within EPCISDocument".

epcis:creationDate_IsMandatory
    sh:path dcterms:created ;
    sh:minCount 1;
    sh:maxCount 1;
    sh:datatype  xsd:dateTime;
    sh:message "creationDate must be specified within EPCISDocument".
     
epcis:epcisBodyShape
    a sh:NodeShape;
    sh:targetObjectsOf epcis:epcisBody;
    sh:property epcis:EventList_TypeConstraint .


epcis:epcisBody_IsMandatory
    sh:path epcis:epcisBody ;
    sh:minCount 1;
    sh:maxCount 1;
    sh:node epcis:epcisBodyShape ;
    sh:message "epcisBody must be specified within EPCISDocument".
   



    


epcis:PersistentDispositionShape 
    a sh:NodeShape;
    sh:targetObjectsOf epcis:persistentDisposition ;
    sh:closed true;
    sh:property epcis:Set_TypeConstraint ;
    sh:property epcis:Unset_TypeConstraint .


epcis:PersistentDisposition_TypeConstraint
    sh:path epcis:persistentDisposition ;
    sh:minCount 0;
    sh:maxCount 1;
    sh:node epcis:PersistentDispositionShape ;
    sh:name  "persistentDisposition type constraint" ;
    sh:message "persistentDisposition expects an object containing properties set and/or unset to be populated." .

epcis:Set_TypeConstraint
    sh:path epcis:set ;
    sh:minCount 0;
    sh:nodeKind sh:IRI ;
    sh:name "set type constraint" ;
    sh:message "set must be a sh:IRI." 
.

epcis:Unset_TypeConstraint
    sh:path epcis:unset ;
    sh:minCount 0;
    sh:nodeKind sh:IRI ;
