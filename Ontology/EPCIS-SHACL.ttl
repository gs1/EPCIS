    sh:property epcis:QuantityList_TypeConstraint ;
    sh:property epcis:SourceList_TypeConstraint ;
    sh:property epcis:DestinationList_TypeConstraint ;
    sh:property epcis:SensorElementList_TypeConstraint ;
