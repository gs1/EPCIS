epcis:EPCISDocumentShape
    a sh:NodeShape ;
    sh:targetClass epcis:EPCISDocument ;
    sh:property [
        sh:path epcis:eventList ;
        sh:node epcis:Event_TypeConstraint ;
        sh:node epcis:EventID_Constraint ;
    ] .


epcis:Event_TypeConstraint
    a sh:NodeShape ;
    sh:property [
        sh:path rdf:type ;
        sh:in ( epcis:ObjectEvent epcis:AggregationEvent epcis:AssociationEvent epcis:TransformationEvent epcis:TransactionEvent epcis:EPCISEvent);
        sh:message "eventList must contain valid subclasses of EPCISEvent";
    ] .

epcis:EventID_Constraint
    a sh:NodeShape ;
    sh:message "Must be a blank node or ni or urn URI";
    sh:or (
        [ sh:nodeKind sh:BlankNode ]
        [ sh:nodeKind sh:IRI; sh:pattern "^ni://"]
        [ sh:nodeKind sh:IRI; sh:pattern "^urn:uuid:"]
    ).


