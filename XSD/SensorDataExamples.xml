								<sensorReport type="gs1:MeasurementType-Illuminance" value="800" uom="LUX" deviceID="urn:epc:id:giai:4000001.444" deviceMetadata="https://id.gs1.org/giai/4000001444" rawData="https://example.org/giai/401234599999"/>
							</sensorElement>
						</sensorElementList>
					</extension>
				</extension>
			</ObjectEvent>
			<!-- Case 5 (to be discouraged unless there is a strong reason to do so): transmit time-stamped sensor data. -->
			<ObjectEvent>
				<eventTime>2019-04-02T15:00:00.000+01:00</eventTime>
				<eventTimeZoneOffset>+01:00</eventTimeZoneOffset>
