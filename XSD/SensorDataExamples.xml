<epcis:EPCISDocument xmlns:epcis="urn:epcglobal:epcis:xsd:1" schemaVersion="2.0" creationDate="2019-10-07T15:00:00.000+01:00" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:epcglobal:epcis:xsd:1 EPCglobal-epcis-2_0.xsd" xmlns:gs1="https://gs1.org/voc/" xmlns:example="http://ns.example.com/epcis">
	<EPCISBody>
		<EventList>
			<!--  Case 1: capturing various sensor properties in given time intervals (here: 20 minutes) -->
			<ObjectEvent>
				<eventTime>2019-04-02T15:00:00.000+01:00</eventTime>
				<eventTimeZoneOffset>+01:00</eventTimeZoneOffset>
				<epcList>
					<epc>urn:epc:id:sgtin:4012345.011111.9876</epc>
				</epcList>
				<action>OBSERVE</action>
				<bizStep>urn:epcglobal:cbv:bizstep:inspecting</bizStep>
				<readPoint>
					<id>urn:epc:id:sgln:4012345.00005.0</id>
				</readPoint>
				<extension>
					<extension>
						<sensorElementList>
							<sensorElement>
								<sensorMetaData time="2019-04-02T14:05:00.000+01:00" deviceID="urn:epc:id:giai:4000001.111" deviceMetaData="https://id.gs1.org/giai/4000001111" rawData="https://example.org/giai/401234599999"/>
								<sensorReport type="gs1:Temperature" value="26.0" uom="CEL"/>
								<sensorReport type="gs1:Humidity" value="12.1" uom="A93"/>
								<sensorReport type="gs1:Speed" value="160.0" uom="KMH"/>
								<sensorReport type="gs1:Illuminance" value="800.0" uom="LUX"/>
							</sensorElement>
							<sensorElement>
								<sensorMetaData time="2019-04-02T14:35:00.000+01:00" deviceID="urn:epc:id:giai:4000001.111" deviceMetaData="https://id.gs1.org/giai/4000001111" rawData="https://example.org/giai/401234599999"/>
								<sensorReport type="gs1:Temperature" value="26.1" uom="CEL"/>
								<sensorReport type="gs1:Humidity" value="12.2" uom="A93"/>
								<sensorReport type="gs1:Speed" value="161.0" uom="KMH"/>
								<sensorReport type="gs1:Illuminance" value="801.0" uom="LUX"/>
							</sensorElement>
							<sensorElement>
								<sensorMetaData time="2019-04-02T14:55:00.000+01:00" deviceID="urn:epc:id:giai:4000001.111" deviceMetaData="https://id.gs1.org/giai/4000001111" rawData="https://example.org/giai/401234599999"/>
								<sensorReport type="gs1:Temperature" value="26.2" uom="CEL"/>
								<sensorReport type="gs1:Humidity" value="12.2" uom="A93"/>
								<sensorReport type="gs1:Speed" value="162.0" uom="KMH"/>
								<sensorReport type="gs1:Illuminance" value="802.0" uom="LUX"/>
							</sensorElement>
						</sensorElementList>
					</extension>
				</extension>
			</ObjectEvent>
			<!-- Case 2: based on the same data like case 1, but in a more compact, business-oriented form -->
			<ObjectEvent>
				<eventTime>2019-04-02T15:00:00.000+01:00</eventTime>
				<eventTimeZoneOffset>+01:00</eventTimeZoneOffset>
				<epcList>
					<epc>urn:epc:id:sgtin:4012345.011111.9876</epc>
				</epcList>
				<action>OBSERVE</action>
				<bizStep>urn:epcglobal:cbv:bizstep:inspecting</bizStep>
				<readPoint>
					<id>urn:epc:id:sgln:4012345.00005.0</id>
				</readPoint>
				<extension>
					<extension>
						<sensorElementList>
							<sensorElement>
								<sensorMetaData startTime="2019-04-02T13:55:01.000+01:00" endTime="2019-04-02T14:55:00.000+01:00" deviceID="urn:epc:id:giai:4000001.111" deviceMetaData="https://id.gs1.org/giai/4000001111" rawData="https://example.org/giai/401234599999" bizRules="https://example.com/gdti/4012345000054987"/>
								<sensorReport type="gs1:Temperature" minValue="26.0" maxValue="26.2" uom="CEL" meanValue="26.1", sDev="0.1"/>
								<sensorReport type="gs1:Humidity" minValue="12.1" maxValue="12.2" uom="A93"/>
								<sensorReport type="gs1:Speed" minValue="160.0" maxValue="162.0" uom="KMH"/>
								<sensorReport type="gs1:Illuminance" minValue="800.0" maxValue="802.0" uom="LUX"/>
								<sensorReport type="gs1:molar_concentration" chemicalSubstance="https://identifiers.org/inchikey:CZMRCDWAGMRECN-UGDNZRGBSA-N" value="0.18" uom="C35"/>
								<sensorReport type="gs1:molar_concentration" microorganism="https://www.ncbi.nlm.nih.gov/taxonomy/1126011" value="0.05" uom="C35"/>
							</sensorElement>
						</sensorElementList>
					</extension>
				</extension>
			</ObjectEvent>
			<!-- Case 3: documenting the storage conditions of a given location (note that there is no object ID in the what dimension) -->
			<ObjectEvent>
				<eventTime>2019-04-02T15:00:00.000+01:00</eventTime>
				<eventTimeZoneOffset>+01:00</eventTimeZoneOffset>
				<epcList/>
				<action>OBSERVE</action>
				<bizStep>urn:epcglobal:cbv:bizstep:inspecting</bizStep>
				<readPoint>
					<id>urn:epc:id:sgln:4012345.00005.0</id>
				</readPoint>
				<extension>
					<extension>
						<sensorElementList>
							<sensorElement>
								<sensorMetaData startTime="2019-04-01T15:00:00.000+01:00" endTime="2019-04-02T14:59:59.999+01:00"/>
								<sensorReport type="gs1:Temperature" minValue="12.4" maxValue="13.8" uom="CEL"/>
								<sensorReport type="gs1:Humidity" minValue="69.2" maxValue="72.5" uom="A93"/>
							</sensorElement>
						</sensorElementList>
					</extension>
				</extension>
			</ObjectEvent>
			<!-- Case 4: include some meta data in sensorReport element (in case sensor data originates from various sensor devices) -->
			<ObjectEvent>
				<eventTime>2019-04-02T15:00:00.000+01:00</eventTime>
				<eventTimeZoneOffset>+01:00</eventTimeZoneOffset>
				<epcList/>
				<action>OBSERVE</action>
				<bizStep>urn:epcglobal:cbv:bizstep:inspecting</bizStep>
				<readPoint>
					<id>urn:epc:id:sgln:4012345.00005.0</id>
				</readPoint>
				<extension>
					<quantityList>
						<quantityElement>
							<epcClass>urn:epc:class:lgtin:4023333.002000.2019-10-07</epcClass>
							<quantity>150</quantity>
							<uom>KGM</uom>
						</quantityElement>
					</quantityList>
					<extension>
						<sensorElementList>
							<sensorElement>
								<sensorMetaData time="2019-04-02T14:55:00.000+01:00"/>
								<sensorReport type="gs1:Temperature" value="26.0" uom="CEL" deviceID="urn:epc:id:giai:4000001.111" deviceMetaData="https://id.gs1.org/giai/4000001111" rawData="https://example.org/giai/401234599999"/>
								<sensorReport type="gs1:Humidity" value="12.1" uom="A93" deviceID="urn:epc:id:giai:4000001.222" deviceMetaData="https://id.gs1.org/giai/4000001222" rawData="https://example.org/giai/401234599999"/>
								<sensorReport type="gs1:Speed" value="160" uom="KMH" deviceID="urn:epc:id:giai:4000001.333" deviceMetaData="https://id.gs1.org/giai/4000001333" rawData="https://example.org/giai/401234599999"/>
								<sensorReport type="gs1:Illuminance" value="800" uom="LUX" deviceID="urn:epc:id:giai:4000001.444" deviceMetaData="https://id.gs1.org/giai/4000001444" rawData="https://example.org/giai/401234599999"/>
							</sensorElement>
						</sensorElementList>
					</extension>
				</extension>
			</ObjectEvent>
			<!-- Case 5 (to be discouraged unless there is a strong reason to do so): transmit time-stamped sensor data. -->
			<ObjectEvent>
				<eventTime>2019-04-02T15:00:00.000+01:00</eventTime>
				<eventTimeZoneOffset>+01:00</eventTimeZoneOffset>
				<epcList>
					<epc>urn:epc:id:sgtin:4012345.011111.9876</epc>
				</epcList>
				<action>OBSERVE</action>
				<bizStep>urn:epcglobal:cbv:bizstep:inspecting</bizStep>
				<readPoint>
					<id>urn:epc:id:sgln:4012345.00005.0</id>
				</readPoint>
				<extension>
					<extension>
						<sensorElementList>
							<sensorElement>
								<sensorMetaData deviceID="urn:epc:id:giai:4000001.111" deviceMetaData="https://id.gs1.org/giai/4000001111"/>
								<sensorReport time="2019-04-02T14:05:00.000+01:00" type="gs1:Temperature" value="26.0" uom="CEL"/>
								<sensorReport time="2019-04-02T14:15:00.000+01:00" type="gs1:Temperature" value="26.1" uom="CEL"/>
								<sensorReport time="2019-04-02T14:25:00.000+01:00" type="gs1:Temperature" value="26.2" uom="CEL"/>
								<sensorReport time="2019-04-02T14:35:00.000+01:00" type="gs1:Temperature" value="26.3" uom="CEL"/>
								<sensorReport time="2019-04-02T14:45:00.000+01:00" type="gs1:Temperature" value="26.4" uom="CEL"/>
								<sensorReport time="2019-04-02T14:55:00.000+01:00" type="gs1:Temperature" value="26.5" uom="CEL"/>
							</sensorElement>
						</sensorElementList>
					</extension>
				</extension>
			</ObjectEvent>
			<!-- Case 6: user/vendor extensions -->
			<ObjectEvent>
				<eventTime>2019-10-07T15:00:00.000+01:00</eventTime>
				<eventTimeZoneOffset>+01:00</eventTimeZoneOffset>
				<epcList/>
				<action>OBSERVE</action>
				<bizStep>urn:epcglobal:cbv:bizstep:inspecting</bizStep>
				<readPoint>
					<id>urn:epc:id:sgln:4012345.00005.0</id>
				</readPoint>
				<extension>
					<extension>
						<sensorElementList>
							<sensorElement>
								<sensorMetaData startTime="2019-04-01T15:00:00.000+01:00" endTime="2019-04-02T14:59:59.999+01:00" example:someFurtherMetaData="someText"/>
								<sensorReport type="gs1:Temperature" minValue="12.4" maxValue="13.8" meanValue="13.2" sDev="0.41" uom="CEL" percRank="50" percValue="12.7" example:cv="123"/>
								<sensorReport type="example:someSensorProperty" stringValue="someSensorOutput"/>
								<example:furtherSensorData>
									<example:measure1>123.5</example:measure1>
									<example:measure2>0.987</example:measure2>
								</example:furtherSensorData>
							</sensorElement>
						</sensorElementList>
					</extension>
				</extension>
			</ObjectEvent>
			<!-- Case 7: accommodating non-float values -->
			<ObjectEvent>
				<eventTime>2019-10-07T16:00:00.000+01:00</eventTime>
				<eventTimeZoneOffset>+01:00</eventTimeZoneOffset>
				<epcList>
					<epc>urn:epc:id:sgtin:4012345.011111.9876</epc>
				</epcList>
				<action>OBSERVE</action>
				<bizStep>urn:epcglobal:cbv:bizstep:inspecting</bizStep>
				<readPoint>
					<id>urn:epc:id:sgln:4012345.00005.0</id>
				</readPoint>
				<extension>
					<extension>
						<sensorElementList>
							<sensorElement>
								<sensorMetaData time="2019-07-19T14:00:00.000+01:00"/>
								<sensorReport type="gs1:Temperature" value="26.0" uom="CEL" deviceID="urn:epc:id:giai:4000001.111"/>
								<sensorReport type="rail:Abc" stringValue="111100001111000011110000" deviceID="urn:epc:id:giai:4000001.112"/>
								<sensorReport type="rail:Def" booleanValue="true" deviceID="urn:epc:id:giai:4000001.113"/>
								<sensorReport type="example:Ghi" stringValue="SomeString" deviceID="urn:epc:id:giai:4000001.114"/>
								<sensorReport type="example:Jkl" hexBinaryValue="f0f0f0" deviceID="urn:epc:id:giai:4000001.115"/>
								<sensorReport type="rail:Mno" uriValue="https://example.org/rail/someSectorSpecificValue" deviceID="urn:epc:id:giai:4000001.116"/>
							</sensorElement>
						</sensorElementList>
					</extension>
				</extension>
			</ObjectEvent>
			<!-- Case 8: other eventType, containing ALL possible attributes -->
			<AggregationEvent>
				<eventTime>2019-10-07T15:30:00.000+01:00</eventTime>
				<eventTimeZoneOffset>+01:00</eventTimeZoneOffset>
				<parentID>urn:epc:id:sscc:4012345.0111111111</parentID>
				<childEPCs/>
				<action>ADD</action>
				<bizStep>urn:epcglobal:cbv:bizstep:packing</bizStep>
				<disposition>urn:epcglobal:cbv:disp:in_progress</disposition>
				<readPoint>
					<id>urn:epc:id:sgln:4012345.00025.0</id>
				</readPoint>
				<extension>
					<childQuantityList>
						<quantityElement>
							<epcClass>urn:epc:class:lgtin:4012345.011111.1234</epcClass>
							<quantity>52</quantity>
							<uom>KGM</uom>
						</quantityElement>
					</childQuantityList>
					<extension>
						<sensorElementList>
							<sensorElement>
								<sensorMetaData time="2019-07-19T14:00:00.000+01:00" deviceID="urn:epc:id:giai:4000001.111" deviceMetaData="https://id.gs1.org/giai/4000001111" rawData="https://example.org/giai/401234599999" dataProcessingMethod="https://example.com/gdti/4012345000054987" bizRules="https://example.org/gdti/4012345000054987"/>
								<sensorReport type="gs1:Humidity" value="12.1" uom="A93"/>
								<sensorReport type="gs1:molar_concentration" chemicalSubstance="https://identifiers.org/inchikey:CZMRCDWAGMRECN-UGDNZRGBSA-N" value="0.18" uom="C35"/>
								<sensorReport type="gs1:molar_concentration" microorganism="https://www.ncbi.nlm.nih.gov/taxonomy/1126011" value="0.05" uom="C35"/>
							</sensorElement>
							<sensorElement>
								<sensorMetaData startTime="2019-04-01T15:00:00.000+01:00" endTime="2019-04-02T14:59:59.999+01:00" example:someFurtherMetaData="someText"/>
								<sensorReport type="gs1:Temperature" minValue="12.4" maxValue="13.8" meanValue="13.2" sDev="0.41" uom="CEL" percRank="50" percValue="12.7" example:cv="123"/>
								<sensorReport type="example:someSensorProperty" stringValue="someSensorOutput"/>
								<example:furtherSensorData>
									<measure1>123.5</measure1>
									<measure2>0.987</measure2>
								</example:furtherSensorData>
							</sensorElement>
							<sensorElement>
								<sensorReport type="gs1:Temperature" uom="CEL" time="2019-07-19T14:00:00.000+01:00" deviceID="urn:epc:id:giai:4000001.111" deviceMetaData="https://id.gs1.org/giai/4000001111" rawData="https://example.org/giai/401234599999" dataProcessingMethod="https://example.com/gdti/4012345000054987" bizRules="https://example.org/gdti/4012345000054987"/>
								<sensorReport type="example:someSensorProperty" stringValue="someSensorOutput"/>
							</sensorElement>
						</sensorElementList>
					</extension>
				</extension>
			</AggregationEvent>
			<!-- Case 9: Event with an error condition AND an alarm condition-->
            		<ObjectEvent>
				<eventTime>2020-05-07T16:00:00.000+01:00</eventTime>
				<eventTimeZoneOffset>+01:00</eventTimeZoneOffset>
				<epcList>
					<epc>urn:epc:id:sgtin:4012345.011111.9876</epc>
				</epcList>
				<action>OBSERVE</action>
				<bizStep>urn:epcglobal:cbv:bizstep:sensor_reporting</bizStep>
				<readPoint>
					<id>urn:epc:id:sgln:4012345.00005.0</id>
				</readPoint>
				<extension>
					<extension>
						<sensorElementList>
							<sensorElement>
								<sensorReport type="gs1:ErrorCondition" uriValue="https://example.com/ErrorCode-A827"/>
                                				<sensorReport type="gs1:AlarmCondition" booleanValue="true"/>
							</sensorElement>
						</sensorElementList>
					</extension>
				</extension>
			</ObjectEvent>
            <!-- Case 10: Event with multi-dimensional sensor data
            Here: velocity, consisting of three scalar values for speed (in m/s), for each direction of motion -->
            		<ObjectEvent>
				<eventTime>2020-05-08T15:00:00.000+01:00</eventTime>
				<eventTimeZoneOffset>+01:00</eventTimeZoneOffset>
				<epcList>
					<epc>urn:epc:id:sgtin:4012345.022222.1234</epc>
				</epcList>
				<action>OBSERVE</action>
				<bizStep>urn:epcglobal:cbv:bizstep:sensor_reporting</bizStep>
				<readPoint>
					<id>urn:epc:id:sgln:4012345.00005.0</id>
				</readPoint>
				<extension>
					<extension>
						<sensorElementList>
							<sensorElement>
								<sensorReport type="gs1:Speed" value="4.5" component="example:x" uom="MTS"/>
                                				<sensorReport type="gs1:Speed" value="12.8" component="example:y" uom="MTS"/>
                                				<sensorReport type="gs1:Speed" value="0" component="example:z" uom="MTS"/>
							</sensorElement>
						</sensorElementList>
					</extension>
				</extension>
			</ObjectEvent>
			<!-- Case 11: Transation Event with sensor data, here: effective dose rate (P71 = millisievert per hour) -->
			<TransactionEvent>
				<eventTime>2020-07-03T00:05:00-06:00</eventTime>
				<eventTimeZoneOffset>-06:00</eventTimeZoneOffset>
				<bizTransactionList>
					<bizTransaction type="urn:epcglobal:cbv:btt:po">urn:epcglobal:cbv:bt:4012345123456:RE100099</bizTransaction>
				</bizTransactionList>
				<epcList>
					<epc>urn:epc:id:sgtin:0614141.107340.1</epc>
					<epc>urn:epc:id:sgtin:0614141.107340.2</epc>
				</epcList>
				<action>ADD</action>
				<bizStep>urn:epcglobal:cbv:bizstep:inspecting</bizStep>
				<disposition>urn:epcglobal:cbv:disp:needs_replacement</disposition>
				<readPoint>
					<id>urn:epc:id:sgln:4012345.00000.5</id>
				</readPoint>
				<extension>
					<extension>
						<sensorElementList>
							<sensorElement>
								<sensorReport type="gs1:EffectiveDoseRate" value="0.005" uom="P71"/>
							</sensorElement>
						</sensorElementList>
					</extension>
				</extension>
			</TransactionEvent>
		</EventList>
	</EPCISBody>
</epcis:EPCISDocument>
