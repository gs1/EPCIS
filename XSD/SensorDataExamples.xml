<epcis:EPCISDocument xmlns:epcis="urn:epcglobal:epcis:xsd:1" schemaVersion="2.0" creationDate="2019-10-07T15:00:00.000+01:00" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:epcglobal:epcis:xsd:1 EPCglobal-epcis-2_0.xsd" xmlns:gs1="https://gs1.org/voc/" xmlns:cbvmda="urn:epcglobal:cbv:mda" xmlns:example="https://ns.example.com/epcis" xmlns:example1="https://ns.example1.com/epcis" xmlns:example2="https://ns.example2.com/epcis" xmlns:example3="https://ns.example3.com/epcis" xmlns:example4="https://ns.example4.com/epcis">
	<EPCISBody>
		<EventList>
			<!--  Case 1: capturing various sensor properties in given time intervals (here: 20 minutes) -->
			<ObjectEvent>
				<eventTime>2019-04-02T15:00:00.000+01:00</eventTime>
				<eventTimeZoneOffset>+01:00</eventTimeZoneOffset>
				<epcList>
					<epc>urn:epc:id:sgtin:4012345.011111.9876</epc>
				</epcList>
				<action>OBSERVE</action>
				<bizStep>urn:epcglobal:cbv:bizstep:inspecting</bizStep>
				<readPoint>
					<id>urn:epc:id:sgln:4012345.00005.0</id>
				</readPoint>
				<extension>
					<extension>
						<sensorElementList>
							<sensorElement>
								<sensorMetadata time="2019-04-02T14:05:00.000+01:00" deviceID="urn:epc:id:giai:4000001.111" deviceMetadata="https://id.gs1.org/giai/4000001111" rawData="https://example.org/giai/401234599999"/>
								<sensorReport type="gs1:MeasurementType-Temperature" value="26.0" uom="CEL"/>
								<sensorReport type="gs1:MeasurementType-Humidity" value="12.1" uom="A93"/>
								<sensorReport type="gs1:MeasurementType-Speed" value="160.0" uom="KMH"/>
								<sensorReport type="gs1:MeasurementType-Illuminance" value="800.0" uom="LUX"/>
							</sensorElement>
							<sensorElement>
								<sensorMetadata time="2019-04-02T14:35:00.000+01:00" deviceID="urn:epc:id:giai:4000001.111" deviceMetadata="https://id.gs1.org/giai/4000001111" rawData="https://example.org/giai/401234599999"/>
								<sensorReport type="gs1:MeasurementType-Temperature" value="26.1" uom="CEL"/>
								<sensorReport type="gs1:MeasurementType-Humidity" value="12.2" uom="A93"/>
								<sensorReport type="gs1:MeasurementType-Speed" value="161.0" uom="KMH"/>
								<sensorReport type="gs1:MeasurementType-Illuminance" value="801.0" uom="LUX"/>
							</sensorElement>
							<sensorElement>
								<sensorMetadata time="2019-04-02T14:55:00.000+01:00" deviceID="urn:epc:id:giai:4000001.111" deviceMetadata="https://id.gs1.org/giai/4000001111" rawData="https://example.org/giai/401234599999"/>
