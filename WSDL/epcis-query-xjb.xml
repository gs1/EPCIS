    xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema" jaxb:extensionBindingPrefixes="xjc">
    <jaxb:bindings schemaLocation="../XSD/EPCglobal-epcis-2_0.xsd">
